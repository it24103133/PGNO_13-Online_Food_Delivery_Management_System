<div class="rating-container mb-3" data-rating-type="${param.ratingType}">
    <input type="hidden" name="${param.ratingType}Rating" id="${param.ratingType}Rating" value="0">
    <div class="stars">
        <i class="far fa-star fa-2x" data-value="1"></i>
        <i class="far fa-star fa-2x" data-value="2"></i>
        <i class="far fa-star fa-2x" data-value="3"></i>
        <i class="far fa-star fa-2x" data-value="4"></i>
        <i class="far fa-star fa-2x" data-value="5"></i>
    </div>
    <small class="text-muted ms-2">Click to rate</small>
</div>
